#include <limits.h>

int minStartValue(int* nums, int numsSize) {
    int currentSum = 0;
    int minSum = INT_MAX;
    
    for (int i = 0; i < numsSize; i++) {
        currentSum += nums[i];
        if (currentSum < minSum) {
            minSum = currentSum;
        }
    }
    
    if (minSum >= 1) {
        return 1;
    } else {
        return 1 - minSum;
    }
}
